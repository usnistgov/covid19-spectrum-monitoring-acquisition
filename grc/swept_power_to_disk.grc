import numpy as np
from gnuradio import gr
import pmt


class blk(gr.basic_block):  # other base classes are basic_block, decim_block, interp_block
    """Embedded Python Block example - a simple multiply const"""

    def __init__(self, key='key name', bin_size=100, default=None):  # only default arguments here
        """arguments to this function show up as parameters in GRC"""

        self._decim = int(bin_size)
        gr.basic_block.__init__(
            self,
            name='Tagged Changes to Stream',   # will show up in GRC
            in_sig=[np.float32],
            out_sig=[np.float32],
        )

        if default == '':
            self.value = None
        else:
            self.value = default

        self._key = key

        # # if an attribute with the same name as a parameter is found,
        # # a callback is registered (properties work, too).
        self.set_output_multiple(self._decim)
        self.set_fixed_rate(True)
        self.set_min_output_buffer(self._decim)
        self.set_relative_rate(1)

    def forecast(self, noutput_items, ninput_items_required):
        #setup size of input_items[i] for work call
        for i in range(len(ninput_items_required)):
            ninput_items_required[i] = self._decim

    def general_work(self, input_items, output_items):
        # if len(input_items[0]) &lt; self._decim:
        #     return 0

        # update the current value from tags when available
        tags = self.get_tags_in_range(0,
            self.nitems_read(0),
            self.nitems_read(0)+self._decim,
            pmt.to_pmt(self._key))
        if len(tags)&gt;0:
            if pmt.to_python(tags[-1].key) == self._key:
                self.value = pmt.to_python(tags[-1].value)

        if self.value is None:
            output_items[0][:self._decim] = input_items[:][0]
        else:
            output_items[0][:self._decim] = self.value

        self.consume(0, self._decim)

        return self._decim
